<a name="div-tabs"></a>
<div id="div-tabs">
  <div class="row">
    <div class="medium-1 large-1 columns">&nbsp;</div>
    <div class="medium-10 large-10 columns">
      <div style="border-bottom: 1px solid #ddd; position: relative;">
        <a href="#div-tabs" id="tab-wikipedia" class="small button alert" style="border-radius: 0; border-left-color: white; border-left-width: 2px; display: table-cell; vertical-align: bottom;"><i class="fi-web"></i> Wikipedia Entry</a>
        <a href="#div-tabs" class="small button" id="tab-flickr" style="border-radius: 0; border-left-color: white; border-left-width: 2px; display: table-cell; vertical-align: bottom;"><i class="fi-social-flickr"></i> Flickr Photos</a>        <a href="#div-tabs" class="small button" id="tab-twitter" style="border-radius: 0; border-left-color: white; border-left-width: 2px; display: table-cell; vertical-align: bottom;"><i class="fi-social-twitter"></i> Latest Twits</a>
      </div>
    </div>
    <div class="medium-1 large-1 columns">&nbsp;</div>
  </div>

  <div class="row">
    <div class="medium-10 large-10 columns" id="wikipediaDiv">
      <br>
      <h6>Fetching Data..</h6>
    </div>
    <div class="medium-1 large-1 columns">&nbsp;</div>
  </div>


  <script id="wikipedia-template" type="text/x-handlebars-template">
    <br/>
    <h3>{{title}}</h3>
    <hr>
    {{#each info}}
      {{{this.innerHTML}}}
      <br/><br/>
    {{/each}}
  </script>




  <div class="row" id="flickrDiv">
    <div class="medium-1 large-1 columns">&nbsp;</div>
    <div class="medium-10 large-10 columns">
      <br>
      <h4>Flickr Photos</h4>
      <hr>
      <div id="flickr_photos"></div>
    </div>
    <div class="medium-1 large-1 columns">&nbsp;</div>
  </div>


<!-- //data.photos.photo[2].id
//data.photos.photo[2].server
//data.photos.photo[2].farm
//data.photos.photo[2].secret
//data.photos.photo[2].owner
-->

<script id="flickr-template" type="text/x-handlebars-template">
  {{#each photos}}
  <div class="medium-3 large-3 columns text-center" style="margin-bottom: 20px;">
    <a class="th" href="../img/demos/demo2.png"><img src="https://farm{{this.farm}}.staticflickr.com/{{this.server}}/{{this.id}}_{{this.secret}}_m.jpg" style="height: 160px;"></a>
    <br>
    <h5><small>{{this.title}}</small></h5>
  </div>
  {{/each}}
</script>

</div>